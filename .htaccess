RewriteCond %{SCRIPT_FILENAME} !-d
RewriteCond %{SCRIPT_FILENAME} !-f

RewriteRule ^(.+)\.html ctrl/index.php?accion=$1 

RewriteRule ^([a-zA-Z0-9_-]+)/(.*)/$ ctrl/index.php?accion=$1&id=$2

SetEnv TZ America/Argentina/Buenos_Aires